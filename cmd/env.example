# Http service config
IS_HTTP=false
SERVER_PORT=8080
LOG_FOLDER=logs
LOG_PRETTY=true
CERT_PEM_FILE=./certificates/cert.pem
KEY_PEM= ./certificates/key.pem
ALLOWED_ORIGINS=*,http://localhost:3000
ALLOWED_METHODS=POST,PUT,GET,DELETE
JWT_SECRET_KEY=your-jwt-secret-key

# Files storage
IMAGES_DIR=./images 

#Database
ENGINE=postgres
DB_USER=postgres
DB_PASSWORD=postgres
DB_HOST=127.0.0.1
DB_PORT=5432
DB_NAME=postgres
DB_SSL_MODE=disable

# Paypal config
WEBHOOK_ID=your-webhook-id
VALIDATION_URL = https://api.sandbox.paypal.com/v1/notifications/verify-webhook-signature
CLIENT_ID=your-client-id
SECRET_ID=your-secret-id